{{ define "lke_cluster_basic" }}

resource "linode_lke_cluster" "test" {
    label       = "{{.Label}}"
    region      = "{{ .Region }}"
    k8s_version = "{{.K8sVersion}}"
    tags        = ["test"]

    pool {
        type  = "g6-standard-1"
        count = 3
        tags  = ["test"]
    }
}

{{ end }}