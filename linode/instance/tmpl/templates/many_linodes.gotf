{{ define "instance_many_linodes" }}

resource "linode_instance" "foobar" {
    count = 12
    label = "{{.Label}}-${count.index}"
    type = "g6-nanode-1"
    image = "{{.Image}}"
    region = "{{ .Region }}"
    root_pass = "terraform-test"
    swap_size = 256
    authorized_keys = ["{{.PubKey}}"]
}

{{ end }}